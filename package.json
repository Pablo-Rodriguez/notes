    "express": "^4.15.4",
    "express-history-api-fallback": "^2.2.1",
    "express-session": "^1.15.5",
    "isomorphic-fetch": "^2.2.1",
    "mixin-es6": "^1.0.1",
    "morgan": "^1.8.2",
    "mysql2": "^1.5.2",
    "nanobus": "^4.3.3",
    "nanocomponent": "^6.5.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "query-string": "^5.1.0",
    "remark": "^9.0.0",
