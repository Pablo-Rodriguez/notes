    "babelify": "^8.0.0",
    "browser-sync": "^2.23.6",
    "browserify": "^16.0.0",
    "chai": "^4.1.2",
    "faker": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-htmlmin": "^4.0.0",
    "gulp-stylus": "^2.7.0",
    "gulp-uglify-es": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "nib": "^1.1.2",
    "nodemon": "^1.11.0",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.14.0",
    "standard": "^10.0.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
